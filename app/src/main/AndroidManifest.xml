<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.txbdc.armmeasurement">

    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.txbdc.armmeasurement.DisplayArmValues">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.txbdc.armmeasurement.DisplayFingerValues" />
        <activity android:name="com.txbdc.armmeasurement.SplashPage" />
        <activity android:name="com.txbdc.armmeasurement.ConnectDevices" />
        <activity android:name="com.txbdc.armmeasurement.ListDevices" />
        <activity android:name="com.txbdc.armmeasurement.References"></activity>
    </application>

</manifest>